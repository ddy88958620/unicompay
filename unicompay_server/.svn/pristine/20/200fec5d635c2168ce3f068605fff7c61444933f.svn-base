package com.unicom.sms.bean;

import org.marker.protocol.sgip.msg.MsgHead;

import java.util.Date;

/**
 *    //TODO
 *
 * @author zhouxw {@link zhouxw@tydic.com} 
 * @version  Loyalty 下午1:53:56
 * @since 1.0
