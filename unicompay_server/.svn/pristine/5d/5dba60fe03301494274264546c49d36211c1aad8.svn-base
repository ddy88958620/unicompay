log4j.appender.CONSOLE=org.apache.log4j.ConsoleAppender
log4j.appender.CONSOLE.Threshold=INFO
log4j.appender.CONSOLE.Encoding=UTF-8
log4j.appender.CONSOLE.Target=System.out
log4j.appender.CONSOLE.layout=org.apache.log4j.PatternLayout
log4j.appender.CONSOLE.layout.ConversionPattern=[DIC] %d{HH:mm:ss} %l%n%m%n

log4j.logger.sms=INFO, sms
log4j.appender.sms=org.apache.log4j.DailyRollingFileAppender
log4j.appender.sms.layout=org.apache.log4j.PatternLayout
log4j.appender.sms.layout.ConversionPattern=%d{yyyy-MM-dd_HH:mm:ss} %m%n
log4j.appender.sms.append=true
log4j.appender.sms.file=${catalina.base}/logs/sms_record.log
log4j.appender.sms.MaxBackupIndex=60


log4j.logger.sms_error=INFO, sms_error
log4j.appender.sms_error=org.apache.log4j.DailyRollingFileAppender
log4j.appender.sms_error.layout=org.apache.log4j.PatternLayout
log4j.appender.sms_error.layout.ConversionPattern=%d{yyyy-MM-dd_HH:mm:ss} %m%n
log4j.appender.sms_error.append=true
log4j.appender.sms_error.file=${catalina.base}/logs/error_sms_record.log
log4j.appender.sms_error.MaxBackupIndex=60

log4j.logger.send_sms=INFO, send_sms
log4j.appender.send_sms=org.apache.log4j.DailyRollingFileAppender
log4j.appender.send_sms.layout=org.apache.log4j.PatternLayout
log4j.appender.send_sms.layout.ConversionPattern=%d{yyyy-MM-dd_HH:mm:ss} %m%n
log4j.appender.send_sms.append=true
log4j.appender.send_sms.file=${catalina.base}/logs/send_sms_record.log
log4j.appender.send_sms.MaxBackupIndex=60


log4j.logger.send_sms_error=INFO, send_sms_error
log4j.appender.send_sms_error=org.apache.log4j.DailyRollingFileAppender
log4j.appender.send_sms_error.layout=org.apache.log4j.PatternLayout
log4j.appender.send_sms_error.layout.ConversionPattern=%d{yyyy-MM-dd_HH:mm:ss} %m%n
log4j.appender.send_sms_error.append=true
log4j.appender.send_sms_error.file=${catalina.base}/logs/error_send_sms_record.log
log4j.appender.send_sms_error.MaxBackupIndex=60


